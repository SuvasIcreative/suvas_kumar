<?xml version= "1.0" encoding= "UTF-8"?>
<odoo>
    <data>
        <record id="view_sale_order_form_modify" model="ir.ui.view">
            <field name="name">sale_order_form</field>
            <field name="model">sale.order</field>
            <field name="inherit_id" ref="sale.view_order_form"/>
            <field name="arch" type="xml">
                <field name='email_id' position="before">
                    <field name="customer_rank"/>
                    <field name="stare" widget="priority"/>
                </field>
                <xpath expr="//group/group/field[@name='partner_id']" position="after">

                </xpath>
            </field>
        </record>
    </data>
</odoo>
<odoo>
    <data>
        <!--        form views-->
        <record id="rambo.form" model="ir.ui.view">
            <field name="name">rambo form</field>
            <field name="model">rambo.rambo</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                            <group>
                                <field name="name"/>
                                <field name="dob"/>
                                <field name="gender"/>
                                <field name="user"/>
                                <field name="value2"/>
                                <field name="user1"/>

                            </group>
                            <group>
                                <field name="description"/>
                                <field name="create_date"/>
                                <field name="refer"/>
                                <field name="bool"/>
                                <field name="bool"/>

                            </group>
                            <field name="mod_ids"/>
                            <field name="mod_ids1"/>
                        </group>
                        <notebook>
                            <psge string="Nikhar">
                                <sheet>
                                    <group>
                                        <field name="notes"/>


                                    </group>
                                </sheet>
                            </psge>
                            <psge string="Aryan">
                                <sheet>
                                    <group>
                                        <field name="file"/>
                                    </group>
                                </sheet>

                            </psge>
                        </notebook>
                    </sheet>
                </form>
            </field>
        </record>

        <!-- explicit list view definition -->
        <record id="rambo_tree" model="ir.ui.view">
            <field name="name">rambo_tree</field>
            <field name="model">rambo.rambo</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="name"/>
                    <field name="dob"/>
                    <field name="gender"/>
                    <field name="user"/>
                    <field name="value2"/>
                    <field name="user1"/>
                    <field name="description"/>
                    <field name="create_date"/>
                </tree>
            </field>
        </record>
        <!-- actions opening views on models -->
        <record model="ir.actions.act_window" id="rambo_action_window">
            <field name="name">rambo window</field>
            <field name="res_model">rambo.rambo</field>
            <field name="view_mode">tree,form,kanban</field>
        </record>
